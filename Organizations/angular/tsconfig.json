{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "organizations": [
        "dist/organizations"
      ],
      "organizations/*": [
        "dist/organizations/*"
      ],
      "organizations-config": [
        "dist/organizations-config"
      ],
      "organizations-config/*": [
        "dist/organizations-config/*"
      ]
    }
  }
}